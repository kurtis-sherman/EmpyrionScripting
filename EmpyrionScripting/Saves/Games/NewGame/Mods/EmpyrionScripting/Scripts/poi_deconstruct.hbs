{{! POI-deconstruct blocks no looting}}
{{~scroll 35 10}}{{#entitiesbyname '*'}}
{{#each .}}
{{#test @root.E.Id 'neq' Id}}
<size=3><color=orange>{{Name}}:</color></size>
<size=2>{{#deconstruct . '1-Dump*'}}
  min: {{MinPos}}
  max: {{MaxPos}}
  current X: {{X}}
  current Y: {{Y}}
  current Z: {{Z}}
  TotalBlocks: {{TotalBlocks}}
  CheckedBlocks: {{CheckedBlocks}}
  RemovedBlocks: {{RemovedBlocks}}
</size>
{{else}}
- {{/deconstruct}}
{{/test}}
{{/each}}
{{/entitiesbyname}}{{/scroll}}
